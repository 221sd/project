














/*每个页面公共css */
.container{
	width: 100%;
	height: calc(100vh--70px);
	overflow: hidden;
}
.container scroll-view{
	height: 100%;
}
.fixbg{
	width: 100%;
	height: 100%;
	position: fixed;
	left: 0;
	top: 0;
	background-size: cover;
	background-position: 0 center;
	-webkit-filter: blur(10px);
	        filter: blur(10px);
	-webkit-transform: scale(1.2);
	        transform: scale(1.2);
}

